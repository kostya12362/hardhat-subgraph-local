{
  "name": "auto-listing",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "compile": "mustache ./config.json subgraph.template.yaml > subgraph.yaml && graph codegen && graph build",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy:prod": "graph deploy --node https://api.studio.thegraph.com/deploy/ auto-listing",
